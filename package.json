{
  "name": "prisma-relation-helper-generator",
  "version": "0.2.2",
  "description": "A Prisma generator to create Eloquent-like model helpers.",
  "author": "Shota Sakumoto",
  "license": "MIT",
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "clean": "rm -rf dist generated-helpers",
    "build": "yarn clean && tsc && cp -R src/templates dist/templates && node ./add-shebang.js",
    "seed": "npx ts-node prisma/seed.ts",
    "generate": "yarn build && yarn prisma generate && yarn lint:fix",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "@prisma/generator-helper": "^5.10.0",
    "ejs": "^3.1.9"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "undici": "^7.8.0",
    "undici-types": "^7.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakumoto-shota/prisma-relation-helper-generator.git"
  },
  "keywords": [
    "prisma",
    "generator",
    "eloquent",
    "relation",
    "typescript"
  ]
}
